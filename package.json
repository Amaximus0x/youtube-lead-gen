		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.14",
		"typescript": "^5.9.2",
		"vite": "^7.1.7"
	},
	"dependencies": {
		"@sparticuz/chromium": "^141.0.0",
		"puppeteer": "^24.25.0",
		"puppeteer-core": "^24.25.0"
	}
